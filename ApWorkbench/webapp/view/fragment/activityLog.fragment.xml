<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Activity Log" class="sapUiSizeCompact dialogStyle" contentWidth="25%" contentHeight="100%" showHeader="false">
		<customHeader>
			<Bar>
				<contentLeft>
					<Title text="Activity Log"/>
				</contentLeft>
				<contentRight>
					<Button icon="sap-icon://decline" press="fncloseActivityLog"/>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<HBox class="sapUiSizeCompact sapUiSmallMarginBegin sapUiSmallMarginTop">
				<core:Icon src="sap-icon://circle-task-2" class="openBlockStatus"/>
				<VBox class="dashLine"></VBox>
				<VBox class="poSearchactivitylogVBOX sapUiSmallMarginBottom sapUiTinyMarginBegin" width="80%">
					<Label text="Process Start" design="Bold" class="statusHeader"/>
					<HBox>
						<Label text="Creation Date :"/>
						<Text text="20.05.2021" class="sapUiSmallMarginBegin"/>
					</HBox>
				</VBox>
			</HBox>
			<HBox class="sapUiSizeCompact sapUiSmallMarginBegin">
				<core:Icon src="sap-icon://circle-task-2" class="ConfirmationstatusCSS"/>
				<VBox class="dashLine"></VBox>
				<VBox class="poSearchactivitylogVBOX sapUiSmallMarginBottom sapUiTinyMarginBegin" width="80%">
					<Label text="Accounting Clerk" design="Bold"/>
					<HBox>
						<Label text="Status :"/>
						<Text text="Approve" class="sapUiSmallMarginBegin"/>
					</HBox>
					<HBox>
						<Label text="Date :"/>
						<Text text="21.05.2021" class="sapUiSmallMarginBegin"/>
					</HBox>
				</VBox>
			</HBox>
			<HBox class="sapUiSizeCompact sapUiSmallMarginBegin">
				<core:Icon src="sap-icon://circle-task-2" class="GoodsReceiptCSS"/>
				<VBox class="dashLine"></VBox>
				<VBox class="poSearchactivitylogVBOX sapUiSmallMarginBottom sapUiTinyMarginBegin" width="80%">
					<Label text="Buyer" design="Bold"/>
					<HBox>
						<Label text="Status :"/>
						<Text text="Approve" class="sapUiSmallMarginBegin"/>
					</HBox>
					<HBox>
						<Label text="Date :"/>
						<Text text="22.05.2021" class="sapUiSmallMarginBegin"/>
					</HBox>
				</VBox>
			</HBox>
			<HBox class="sapUiSizeCompact sapUiSmallMarginBegin">
				<core:Icon id="InvoiceReceiptIcon" src="sap-icon://circle-task-2" class="GoodsReceiptCSS"/>
				<VBox class="poSearchactivitylogVBOX sapUiSmallMarginBottom sapUiTinyMarginBegin" width="80%">
					<Label text="Accounting Clerk" design="Bold"/>
					<HBox>
						<Label text="Status :"/>
						<Text text="Close" class="sapUiSmallMarginBegin"/>
					</HBox>
					<HBox>
						<Label text="Date :"/>
						<Text text="22.05.2021" class="sapUiSmallMarginBegin"/>
					</HBox>
				</VBox>
			</HBox>
		</content>
	</Dialog>
</core:FragmentDefinition>