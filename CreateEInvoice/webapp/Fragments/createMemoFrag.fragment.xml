<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified"
	xmlns:f="sap.ui.layout.form">
	<Dialog class="sapUiSizeCompact RDSdialogBg" showHeader="true" title="{i18n>MemoType}">
		<content>
			<RadioButtonGroup id="rbg5" columns="1" select="changeType">
				<RadioButton id="RB51" text="{i18n>Invoice}"/>
				<RadioButton id="RB52" text="{i18n>Credit}"/>
				<RadioButton id="RB43" text="{i18n>Debit}"/>
			</RadioButtonGroup>
		</content>
		<beginButton>
			<Button text="{i18n>OK}" press="OKCreditMemo" class="RDSsecondaryBtnDailog"></Button>
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" press="cancelCreditMemo" class="RDSsecondaryBtnDailog"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>